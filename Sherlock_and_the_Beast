#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int no_tests, no_5groups = 0, invalid = -1;
    int threeSetsofFive = 555;
    int fiveSetsofThree = 33333;
    cin >> no_tests;
    
    // Iterate through test cases
    for(int a0 = 0; a0 < no_tests; a0++){
        int num;
        cin >> num;
        
        if (num < 3)  cout << invalid << "\n";
        
        else{
        if (num%3 == 0){
            for (int i = 0; i < num/3; i++)
                cout << threeSetsofFive;
            cout << "\n";
        } 
        else if (num%5 == 0){
            for (int i = 0; i < num/5; i++)
                cout << fiveSetsofThree;
            cout << "\n";
        } 
        else {
       // Obtain max. number of 5's if "decent-number" is available
        for (int n = 1; n < num; n++){
            if (num-(3*n) < 5) break;
            if ((num-(3*n)) % 5 == 0)
             no_5groups = n;
        }   
            
        if (no_5groups != 0){
            for (int i = 0; i< no_5groups; i++)
                cout << threeSetsofFive;
            for (int i = 0; i < (num-no_5groups*3)/5; i++)
                cout << fiveSetsofThree;
            cout << "\n"; no_5groups = 0;
            }
        else
            cout << invalid << "\n";
        }
      }
    }
    return 0;
}

/* Notes
if < 3, enter -1
if multiple of 3 and 5 (i.e 15) fill w/ 5's
if multiple of 3 only, fill w/ 5's
if multiple of 5 only, fill w/ 3's
else // combination 
    if greater than 5 and NOT multiple of 3/5
    if (N-3) % 5 == 0
        // for loop for n
        if (N-3n) % 5 == 0
        int noFivegroups = n;
*/
